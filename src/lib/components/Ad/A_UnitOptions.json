{
    "StickyBottom": {
        "refreshLimit": 20,
        "refreshTime": 30,
        "format": "anchor",
        "anchor": "bottom",
        "anchorBgColor": "transparent"
    },
    "Article": {
        "refreshLimit": 20,
        "refreshTime": 30,
        "format": "article",
        "pageInterval": 1
    },
    "Video": {
        "format": "video-nc",
        "video": {
            "float": "always"
        },
        "mediaQuery": "(min-width: 1025px)"
    },
    "Banner": {
        "refreshTime": 30,
        "renderVisibleOnly": true,
        "sizes": [
            [
                "728",
                "90"
            ],
            [
                "970",
                "90"
            ],
            [
                "320",
                "50"
            ],
            [
                "320",
                "100"
            ]
        ],
        "report": {
            "enabled": true,
            "icon": true,
            "wording": "Report Ad",
            "position": "top-right"
        }
    },
    "Gutter": {
        "refreshTime": 30,
        "format": "sticky-stack",
        "stickyStackLimit": 15,
        "stickyStackSpace": 2.5,
        "stickyStackOffset": 25,
        "sizes": [],
        "report": {
            "enabled": true,
            "icon": true,
            "wording": "Report Ad",
            "position": "top-right"
        }
    }
}